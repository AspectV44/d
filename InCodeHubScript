		tp(game.Players[v.Name], game.Players.LocalPlayer)
		wait(0.2)
		tp(game.Players[v.Name], game.Players.LocalPlayer)
		wait(0.5)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = NOW
		wait(0.5)
		getout(game.Players.LocalPlayer, game.Players[v.Name])
		wait(0.3)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = NOW
		end
	end)
	
