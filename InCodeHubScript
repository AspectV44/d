	game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
	sound:Play()
end)


PlayerSection:NewSlider("Gravity", "Changes Game's Gravity, Only affects player.", 196.2, 20, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Workspace.Gravity = s
	sound:Play()
end)



