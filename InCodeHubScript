        end
    end
end

spinning = false
Commands.spin = function(args)
    if args[1] then
        for i,v in pairs(findSinglePlayer(args[1])) do
            run(commandPrefix.."attach "..v.Name)
            annplr = v
            annoying = true
            spinning = true
        end
    end
end

Commands.unspin = function(args)
    if spinning then
        annoying = false
        spinning = false
    end
    run(""..commandPrefix.."unattach")
end

