	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.0199530572, 0, 0.090780139, 0)
	TextLabel.Size = UDim2.new(0, 160, 0, 38)
	TextLabel.Font = Enum.Font.SourceSansLight
	TextLabel.Text = "Welcome "..game.Players.LocalPlayer.DisplayName.." ("..game.Players.LocalPlayer.Name..")"
	TextLabel.TextColor3 = Color3.fromRGB(199, 199, 199)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 14.000
	TextLabel.TextWrapped = true

	Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
	wait(3)
	Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)


	wait(2)
	local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
	local Window = Library.CreateLib("InCode Meepcity | Made by ItzDanny, Modified By Loadstring", "BloodTheme")
	local Tab = Window:NewTab("Scripts")
	local Section = Tab:NewSection("Scripts")
	Section:NewKeybind("ToggleKeybind", "Opens and closes Gui", Enum.KeyCode.F, function()
		Library:ToggleUI()
	end)


	Section:NewButton("Plus Gamepass", "Gives you plus gamepass", function()
		game.ReplicatedStorage.PlayerData[game.Players.LocalPlayer.UserId].PLUS.Value = true
		TextLabel.Text = "Gamepass Given"
		Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
		wait(1.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)

	end)

	Section:NewButton("Chalk Gamepass(broken)", "Gives you Chalk gamepass", function()
		print("Its broken noob stop clicking >:(")
		TextLabel.Text = "STOP CLICKING ITS BROKEN "..game.Players.LocalPlayer.Name
		Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
		wait(1.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)
	end)

	Section:NewButton("Radio Gamepass", "Gives you Radio gamepass", function()
