		Gui.Name = HttpService:GenerateGUID(false):gsub('-', ''):sub(1, math.random(25, 30))
		ProtectInstance(Gui);
		Gui.Parent = Parent or CoreGui
		Guis[#Guis + 1] = Gui
		return Gui
	end
	UI = game:GetObjects("rbxassetid://6167929302")[1]:Clone()

