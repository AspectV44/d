    if args[1] then
        for i,v in pairs(findSinglePlayer(args[1])) do
            local Anim = Instance.new("Animation")
            Anim.AnimationId = "rbxassetid://179224234"
            RIDEtrack = LP.Character.Humanoid:LoadAnimation(Anim)
            rideACTIVE = true
            ridePLAYER = v
            RIDEtrack:Play()
        end
    end
end

