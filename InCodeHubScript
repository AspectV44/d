		wait(1.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)
	end)

	Section:NewButton("Money Script", "Look at the name Duh", function()
		game.ReplicatedStorage.Connection:InvokeServer(9,2) --ItzDanny
		game.ReplicatedStorage.Connection:InvokeServer(50)
		game.ReplicatedStorage.PlayerData[game.Players.LocalPlayer.UserId].PLUS.Value = true -- this will give you more money when you sell
		while wait() do
			game.ReplicatedStorage.Connection:InvokeServer(49)
			game.ReplicatedStorage.Connection:InvokeServer(51)
		end 
		TextLabel.Text = "Money Farm Started"
		Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
		wait(1.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)
	end)

	if game.PlaceId ~= 370731277 then



		TextLabel.Text = "Warning: This game isnt MeepCity >:("
		Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
		wait(3.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)
	else
		TextLabel.Text = "This game is meep city, Have fun exploiting >:)"
		Frame:TweenPosition(UDim2.new(0.909, 0,0.929, 0),"InOut","Sine",0.3)
		wait(3.5)
		Frame:TweenPosition(UDim2.new(0.999999999, 0, 0.934000015, 0),"InOut","Sine",0.3)

	end


end)
